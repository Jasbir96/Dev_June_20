{
    "name": "Dev extension",
    "description": "Gives a demo",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "myStyles.css"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "images/*.jpg",
        "images/*.jpeg"
    ]
    
}